<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidroomfirebase.app.quickroom">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="QuickRoom"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".SelectRoomType" />
        <activity android:name=".Eblock" />
        <activity android:name=".Room" />
        <activity android:name=".BlockList" />
        <activity android:name=".RoomList"/>
        <activity android:name=".UploadImage"/>

        </application>

</manifest>